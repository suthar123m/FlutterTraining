/*
3. WAP to take 10 no. Input from user find out below values
a. How many Even numbers are there
*/
import 'dart:io';

void main() {
  int i, even_count = 0;
  int num;
  print("Enter the value of number == ");
  num = int.parse(stdin.readLineSync()!);
  for (i = 1; i <= num; i++) {
    if (i % 2 == 0) {
      even_count++;
    }
  }
  print("Count of even number == $even_count ");
}
